pragma solidity ^0.8.19;

import {Test, console} from "forge-std/Test.sol";
import {MoodMonkeyNft} from "../../src/MoodMonkeyNft.sol";
import {DeployMoodMonkeyNft} from "../../script/DeployMoodMonkeyNft.s.sol";

contract MoodMonkeyNftIntegrationTest is Test {
    MoodMonkeyNft moodMonkeyNft;
    string public constant HAPPY_SVG_IMAGE_URI =
        "data:image/svg+xml;base64,";
    string public constant SAD_SVG_IMAGE_URI =
        "data:image/svg+xml;base64,";
    string public constant SAD_SVG_URI =
        "data:application/json;base64,";
    DeployMoodMonkeyNft deployer;

    address USER = makeAddr("user");

    function setUp() public {
        deployer = new DeployMoodMonkeyNft();
        moodMonkeyNft = deployer.run();
    }

    function testViewTokenURIIntegration() public {
        vm.prank(USER);
        moodMonkeyNft.mintNft();
        // console.log(moodMonkeyNft.tokenURI(0));
    }

    function testFlipTokenToSad() public {
        vm.prank(USER);
        moodMonkeyNft.mintNft();

        vm.prank(USER);
        moodMonkeyNft.flipMood(0);

        console.log("tokenURI(0) is:");
        console.log(moodMonkeyNft.tokenURI(0));
        console.log("and then sad svg:");
        console.log(SAD_SVG_URI);
        assertEq(moodMonkeyNft.tokenURI(0), SAD_SVG_URI);
    }
}
